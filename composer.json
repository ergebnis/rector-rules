{
  "name": "ergebnis/rector-rules",
  "description": "Provides rules for rector/rector.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/rector-rules",
  "support": {
    "issues": "https://github.com/ergebnis/rector-rules/issues",
    "source": "https://github.com/ergebnis/rector-rules",
    "security": "https://github.com/ergebnis/rector-rules/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
    "nikic/php-parser": "^4.17.1 || ^5.0.0",
    "phpstan/phpstan": "^1.10.36",
    "rector/rector": "~0.19.2",
    "symplify/rule-doc-generator-contracts": "^9.3.26"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42.0",
    "ergebnis/data-provider": "^3.2.0",
    "ergebnis/license": "^2.4.0",
    "ergebnis/php-cs-fixer-config": "^6.22.0",
    "ergebnis/phpunit-slow-test-detector": "^2.10.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^9.6.16",
    "psalm/plugin-phpunit": "~0.18.4",
    "vimeo/psalm": "^5.21.1"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Rector\\Rules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Rector\\Rules\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
