{
  "name": "ergebnis/rector-rules",
  "description": "Provides rules for rector/rector.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/rector-rules",
  "support": {
    "issues": "https://github.com/ergebnis/rector-rules/issues",
    "source": "https://github.com/ergebnis/rector-rules",
    "security": "https://github.com/ergebnis/rector-rules/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "nikic/php-parser": "^4.17.1 || ^5.0.0",
    "phpstan/phpstan": "^1.10.36",
    "rector/rector": "^1.0.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.45.0",
    "ergebnis/data-provider": "^3.4.0",
    "ergebnis/license": "^2.6.0",
    "ergebnis/php-cs-fixer-config": "^6.40.0",
    "ergebnis/phpunit-slow-test-detector": "^2.17.0",
    "infection/infection": "~0.26.6",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpstan/phpstan-strict-rules": "^1.6.1",
    "phpunit/phpunit": "^9.6.20"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Rector\\Rules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Rector\\Rules\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
